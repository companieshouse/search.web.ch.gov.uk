{
  "name": "search.web.ch.gov.uk",
  "version": "1.0.0",
  "description": "Search Web for CH.GOV.UK",
  "main": "index.js",
  "scripts": {
    "start": "node dist/bin/www.js",
    "start:watch": "NODE_ENV=development nodemon",
    "build": "NODE_ENV=production tsc && cp -r src/views dist/ && gulp static",
    "lint": "eslint 'src/*.ts' 'src/**/*.ts' ./'test/*.ts' './test/**/*.ts'",
    "lint:fix": "eslint 'src/*.ts' 'src/**/*.ts' './test/*.ts' './test/**/*.ts' --fix",
    "test": "mocha test/**/*.test.ts",
    "test:coverage": "nyc npm test",
    "sonarqube-pull-request": "sonar-scanner -D sonar.pullrequest.base=master",
    "sonarqube": "branch=$(git rev-parse --symbolic-full-name --abbrev-ref HEAD); if [[ $branch == \"HEAD\" ]]; then echo $branch && npm run sonarqube-base-branch; else echo $branch && npm run sonarqube-pull-request; fi;",
    "chs-dev": "nodemon --legacy-watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@companieshouse/api-sdk-node": "^2.0.322",
    "@companieshouse/ch-node-utils": "^2.1.12",
    "@companieshouse/node-session-handler": "^5.2.8",
    "@companieshouse/structured-logging-node": "^2.0.6",
    "@companieshouse/web-security-node": "^4.4.14",
    "cookie-parser": "~1.4.7",
    "cookies": "^0.8.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.4",
    "express-actuator": "^1.8.4",
    "express-validator": "^6.14.0",
    "govuk-frontend": "5.11.0",
    "http-errors": "~2.0.0",
    "ioredis": "~5.3.2",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.1",
    "nunjucks": "~3.2.4",
    "nyc": "^15.1.0",
    "papaparse": "^5.3.2",
    "uuid": "^3.4.0",
    "yargs": "~17.7.2"
  },
  "devDependencies": {
    "@types/chai": "~4.3.5",
    "@types/chai-as-promised": "~7.1.5",
    "@types/chai-http": "^4.2.4",
    "@types/cheerio": "~0.22.31",
    "@types/cookies": "^0.7.4",
    "@types/express": "~4.17.17",
    "@types/express-actuator": "^1.8.0",
    "@types/http-errors": "~2.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "~7.0.2",
    "@types/node": "^12.7.8",
    "@types/nunjucks": "^3.1.2",
    "@types/papaparse": "^5.3.2",
    "@types/proxyquire": "~1.3.28",
    "@types/sinon": "~9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "chai": "~4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "cheerio": "~1.0.0-rc.12",
    "eslint": "8.57.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
