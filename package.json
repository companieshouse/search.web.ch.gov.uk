{
  "name": "web-starter-nodejs",
  "version": "1.0.0",
  "description": "A starter web application",
  "main": "index.js",
  "scripts": {
    "start": "node dist/bin/www.js",
    "start:watch": "NODE_ENV=development nodemon",
    "build": "NODE_ENV=production tsc && cp -r src/views dist/ && gulp static",
    "lint": "eslint 'src/*.ts' 'src/**/*.ts' ./'test/*.ts' './test/**/*.ts'",
    "lint:fix": "eslint 'src/*.ts' 'src/**/*.ts' './test/*.ts' './test/**/*.ts' --fix",
    "test": "mocha test/**/*.test.ts",
    "test:coverage": "nyc npm test",
    "analyse-code": "sonar-scanner"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@companieshouse/api-sdk-node": "1.0.97",
    "@companieshouse/node-session-handler": "~4.1.6",
    "@companieshouse/structured-logging-node": "1.0.4",
    "cookie-parser": "~1.4.4",
    "cookies": "^0.8.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "govuk-frontend": "^3.11.0",
    "http-errors": "~1.7.3",
    "ioredis": "~4.16.0",
    "js-yaml": "^3.14.0",
    "moment": "^2.29.1",
    "nunjucks": "^3.2.1",
    "nyc": "^15.1.0",
    "papaparse": "^5.3.1",
    "uuid": "^3.4.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/chai": "~4.2.11",
    "@types/chai-http": "~4.2.0",
    "@types/cheerio": "~0.22.18",
    "@types/cookies": "^0.7.4",
    "@types/express": "^4.17.6",
    "@types/http-errors": "~1.6.3",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "~7.0.2",
    "@types/node": "^12.7.8",
    "@types/nunjucks": "^3.1.2",
    "@types/papaparse": "^5.3.1",
    "@types/sinon": "~9.0.4",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "cheerio": "~1.0.0-rc.3",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.0.2",
    "mocha": "~7.2.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.3",
    "nyc": "^15.1.0",
    "papaparse": "^5.3.1",
    "sinon": "~9.0.2",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "^8.4.1",
    "typescript": "~3.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "./src"
    ],
    "ext": "ts",
    "legacyWatch": true,
    "ignore": [
      "**/*.test.ts"
    ],
    "exec": "ts-node ./src/bin/www.ts"
  }
}
